{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "max-len": 0,
    "jsx-a11y/no-static-element-interactions": 1,
    "jsx-a11y/no-noninteractive-element-to-interactive-role": 1,
    "react/jsx-filename-extension": 0,
    "react/no-array-index-key": 0
  }
}
